{
  "name": "jj-stretch-monorepo",
  "version": "1.0.5",
  "private": true,
  "description": "JJ Stretch VSCode Extension - Monorepo",
  "scripts": {
    "build": "nx run-many -t build",
    "build:webview": "nx build webview",
    "build:extension": "nx build extension",
    "compile": "nx compile extension",
    "dev:webview": "pnpm --filter webview dev",
    "watch:extension": "pnpm --filter extension watch",
    "package": "nx compile extension && pnpm --filter extension build",
    "publish": "pnpm package && pnpm --filter extension publish"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977",
  "devDependencies": {
    "@nx/js": "^22.2.3",
    "nx": "^22.2.3"
  }
}
